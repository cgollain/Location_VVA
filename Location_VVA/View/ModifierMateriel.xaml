<Window x:Class="Location_VVA.View.ModifierMateriel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ModifierMateriel" Height="300" Width="400">
    <Grid>
        <Button Content="Retour" Grid.Row="0" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Retour_Click"/>

        <TextBox x:Name="textBoxNomMateriel" Margin="108,83,38,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxPrixParJour" Margin="108,124,38,0" VerticalAlignment="Top"/>
        <Button Content="Enregistrer" Click="Enregistrer_Click" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="154,0,0,90" RenderTransformOrigin="1.451,0.749"/>
        <Label Content="Nom Materiel :  " HorizontalAlignment="Left" Height="36" Margin="10,76,0,0" VerticalAlignment="Top" Width="93"/>
        <Label Content="Prix par jour : " HorizontalAlignment="Left" Height="36" Margin="13,118,0,0" VerticalAlignment="Top" Width="98"/>
    </Grid>
</Window>
